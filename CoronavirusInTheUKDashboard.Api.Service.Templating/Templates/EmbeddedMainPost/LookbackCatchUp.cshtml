@using Microsoft.AspNetCore.Html
@using System.Globalization;
@using RazorLight
@using CoronavirusInTheUKDashboard.Api.Service.Templating;

@inherits TemplatePage<CoronavirusInTheUKDashboard.Api.Service.Models.Models.Posts.MainPostModel>

Testing data is not available over the weekend. Here is the weekend data.

||**Daily**|**Cumulative**|
:--|--:|--:|
@foreach (var item in Model.LookbackCatchUpResult.Records)
{
<text>@item.Name (@item.Date.ToString("d MMMM", CultureInfo.CreateSpecificCulture("en-US")) - @Enum.GetName(typeof(DayOfWeek), @item.Date.DayOfWeek)) | @HelperFunctions.Format(@item.Daily) | @HelperFunctions.Format(@item.Cumulative)
</text>
}
||
@foreach (var item in Model.LookbackCatchUpNationalResult.Records)
{
<text>@item.Name (@item.Date.ToString("d MMMM", CultureInfo.CreateSpecificCulture("en-US")) - @Enum.GetName(typeof(DayOfWeek), @item.Date.DayOfWeek)) | @HelperFunctions.Format(@item.Daily) | @HelperFunctions.Format(@item.Cumulative)
</text>
}